<root>
    <in>
        position3f
        normal
    </in>
    <out>
        color3f
    </out>

    <uniform>
        SceneInfo r_scene_info;
        Materials material;
        SunInfo sun;
    </uniform>

    <raw>
        GetNormal
    </raw>

    <module>
        PhongFullyRough
    </module>

    <main>
        out_Color=ApplyDirectionLight(sun.direction,material.diffuse,material.ambient);
        gl_Position=r_scene_info.mvp*vec4(Position,1.0);
    </main>
</root>
